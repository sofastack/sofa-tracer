<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:sofa="http://sofastack.io/schema/sofaboot"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
            http://sofastack.io/schema/sofaboot http://sofastack.io/schema/sofaboot.xsd"
       default-autowire="byName">
    <!-- direct -->
    <bean id="directServiceImpl" class="com.alipay.sofa.tracer.examples.sofarpc.direct.DirectServiceImpl"/>
    <sofa:service ref="directServiceImpl" interface="com.alipay.sofa.tracer.examples.sofarpc.direct.DirectService">
        <sofa:binding.bolt/>
    </sofa:service>
</beans>